
/* This file should compare words and tell how similar they are */

def wordInWord(word1, word2):
  in_counts = 0
  for char in word1:
    if char in word2:
      in_counts += 1
  in_ratio = in_counts / len(word1)

def compareWords(word1, word2):
  ratio_1 = wordInWord(word1, word2)
  ratio_2 = wordInWord(word2, word1)
  return ratio_1 / ratio_2:
    
